<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>ADM_Injection</AssemblyName>
    <RootNamespace>ADM_Injection</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
